import java.util.*;
class Student
{
  int rollno;
  String name;
  int Mark1=0;
  int Mark2=0;
  int Mark3=0;
  int Mark4=0;
  int Mark5=0;
  int Mark6=0;
  static int rank=0;

  float average=0.0f,percentage=0.0f,total=0.0f;
    Student (int a, String b,int c,int d,int e,int f,int g,int h)
  {
    this.rollno = a;
    this.name = b;
    this.Mark1=c;
    this.Mark2=d;
    this.Mark3=e;
    this.Mark4=f;
    this.Mark5=g;
    this.Mark6=h;
    this.total=c+d+e+f+g+h;
    this.average=total/6;
    this.percentage=(total/600)*100;
  }
    public float gettotal() {
        return total;
    }
 
  @Override 
  public String toString ()
  {
     
 return "Sudentdetails:[stu=" +" rank="+(rank=rank+1)+",name="+ name + ",rollno=" + rollno +",total="+total+",percentage="+Math.round(percentage)+",average="+Math.round(average)+"]\n";
  }
}
class   Mytotal implements Comparator<Student> {
   @Override
    public int compare(Student s1, Student s2){
          if(s1.gettotal() < s2.gettotal()){
            return 1;
        } else {
            return -1;
        }
    }
}
public class Main
{
  public static void main (String[]args)
  {
    Scanner scan = new Scanner (System.in);
    System.out.println ("enter the no of student details;");
    int limit = scan.nextInt ();
    Set < Student > arr= new TreeSet <> (new Mytotal());
    int i = 0;
    do
      {
	System.out.println ("Enter " + (++i) + "  student details");
	System.out.println ("rollno | name | Mark1  |Mark2 |Mark3 |Mark4 |Mark5 |Mark6 ");
	int rollno = scan.nextInt ();
	String name= scan.next ();
	int Mark1=scan.nextInt();
    int Mark2=scan.nextInt();
    int Mark3=scan.nextInt();
    int Mark4=scan.nextInt();
    int Mark5=scan.nextInt();
    int Mark6=scan.nextInt();
	Student st = new Student (rollno,name,Mark1,Mark2,Mark3,Mark4,Mark5,Mark6);
	arr.add (st);
    }while (i < limit);

    Iterator itr = arr.iterator ();
    while (itr.hasNext ())
      System.out.print (itr.next ());




  }
}
